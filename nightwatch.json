{
  "src_folders": "test/tests/homePageTest.js",
  "output_folder":"report",
  "selenium": {
    "start_process": false,
    "host": "localhost",
    "port": 4444,
    "cli_args": {
      "webdriver.chrome.driver": "node_modules/selenium-chromedriver/bin/chromedriver.exe",
      "webdriver.ie.driver": "node_modules/iedriver/lib/iedriver/IEDriverServer.exe"
    }
  },
  "test_settings": {
    "default": {
      "selenium_port": 4444,
      "selenium_host": "localhost",
      "silent": true,
      "screenshots": {
        "enabled": true,
        "path": "test/screenshots"
      },
      "desiredCapabilities": {
        "browserName": "chrome",
        "javascriptEnabled": true,
        "acceptSslCerts": true,
        "handlesAlerts":true,
        "cssSelectorsEnabled":true
      },
      "ie": {
        "desiredCapabilities": {
          "browserName": "internet explorer",
          "javascriptEnabled": true,
          "acceptSslCerts": true,
          "handlesAlerts":true
        }
      }
    }
  }
}